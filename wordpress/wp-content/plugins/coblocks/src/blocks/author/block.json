	"attributes": {
		"biography": {
			"type": "array",
			"source": "children",
			"selector": ".wp-block-coblocks-author__biography",
			"default": []
		},
		"name": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-coblocks-author__name"
		},
		"imgId": {
			"type": "number"
		},
		"imgUrl": {
			"type": "string",
			"source": "attribute",
			"attribute": "src",
			"selector": "img"
		},
		"backgroundColor": {
			"type": "string"
		},
		"customBackgroundColor": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		},
		"customTextColor": {
			"type": "string"
		},
		"fontSize": {
			"type": "string"
		},
		"customFontSize": {
			"type": "number"
		}
	}
}
